<?xml version="1.0" encoding="ISO-8859-1"?><!-- edited with XMLSPY v5 rel. 2 U (http://www.xmlspy.com) by Eivind Dahle (Innovative Consulting) --><!-- Edited by Eivind Dahle; åpnet (midlertidig) for at meldingene fra edi-fact løpet skal komme gjennom.--><!-- Edited by Espen Stranger Seland, KITH (www.kith.no)--><!-- This version: 0.66 for final version 2.0--><!-- Copyright 2001-2002 Rikstrygdeverket--><!-- --><!-- Filename: rtv-smlepo-v067.xsd --><!-- Changes since version 0.66: --><!--            Removed "required" status for "trygdekontor" --><!--            Added digital signatur for Legeerklaering (LE) --><!-- Changes since version 0.65: --><!--     New namespace --><!-- Changes since version 0.64: --><!--     Added enumeration value "5" to typeKodesystem, representing ICPC-2. --><!-- Changes since version 0.63: --><!--     Namespace for XMLDsig added --><!--     Removed "logic" from DiagnoseKodesystem. Every element is now optional --><!--     ds:Signature is now optional (0..1) for debugging convenience. --><!-- --><schema targetNamespace="http://www.kith.no/xmlstds/sykmelding/2003-05-01" xmlns:ds="http://www.w3.org/2000/09/xmldsig#" xmlns:rtv="http://www.kith.no/xmlstds/sykmelding/2003-05-01" xmlns:xsd="http://www.w3.org/2001/XMLSchema.xsd" xmlns="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified" attributeFormDefault="unqualified">  <import namespace="http://www.w3.org/2000/09/xmldsig#" schemaLocation="xmldsig-core-schema.xsd"/>  <!--RTV Sykmelding (SM) og Legeerklæring (LE) + Personopplysninger (PO).  -->  <!--= = = = SYKMELDING  = = = =-->  <element name="Sykmelding">    <annotation>      <documentation>Spesialisering av den abstrakte klassen Melding, til klassen Sykmeldingsattest.</documentation>    </annotation>    <complexType>      <sequence>        <element ref="rtv:SykmeldGjelder" minOccurs="0" maxOccurs="4"/>        <element ref="rtv:Pasientopplysninger"/>        <element ref="rtv:Identifikasjon" minOccurs="0"/>        <element ref="rtv:ArbeidsuforhetSykmelding"/>        <element ref="rtv:FriskmeldingPrognose" minOccurs="0"/>        <element ref="rtv:DiagnoseArbeidssituasjon"/>        <element ref="rtv:Merknad" minOccurs="0"/>        <element ref="rtv:Legeunderskrift"/>        <element ref="rtv:MedisinskeOpplysninger" minOccurs="0"/>        <element ref="rtv:Funksjonsvurdering" minOccurs="0"/>        <element ref="ds:Signature" minOccurs="0"/>      </sequence>      <attribute name="formOfLanguage" type="rtv:typeFormOfLanguage" use="required"/>      <attribute name="sendersId" type="string" use="optional"/>      <attribute name="receiversId" type="string" use="optional"/>      <attribute name="schemaVersion" type="string" use="required" fixed="IA 08-07.04 Fastsatt 6.88 Endret 04.2002 Elektronisk XML 2.0"/>      <attribute name="messageId" type="string" use="required"/>      <attribute name="genDate" type="dateTime" use="required"/>    </complexType>  </element>  <element name="SykmeldGjelder">    <annotation>      <documentation>EDA: Satte minOcc til 0 for å få EDIFACT løpet gjennom. For å angi om det er første attest for dette sykmeldingstilfellet, forlengelse av sykmeldingstilfellet, endringer, om del II er benyttet, og/eller om legeerklæring ved sykefravær i mer enn 8 uker er sendt. </documentation>      <documentation>For å angi om det er første attest for dette sykmeldingstilfellet, forlengelse av sykmeldingstilfellet, endringer, om del II er benyttet, og/eller om legeerklæring ved sykefravær i mer enn 8 uker er sendt. </documentation>    </annotation>    <complexType>      <attribute name="typeSykmelding" type="rtv:typeSykmelding" use="required">        <annotation>          <documentation>Kategorisering av om dette er første attest for dette sykdomstilfellet,  om det er en forlengelse, om det er en endring, om del II er benyttet eller om legeerklæring ved sykefravær utover 8 uker er sendt.</documentation>        </annotation>      </attribute>    </complexType>  </element>  <element name="ArbeidsuforhetSykmelding">    <annotation>      <documentation>For å angi første fraværsdag, dato for første konsultasjon, dato for telefonkonsultasjon, arbeidsmuligheter og tiltak. Oopplysninger om varighet angis i klassene Varighet. </documentation>    </annotation>    <complexType>      <sequence>        <element name="ForsteFraversdag" type="date" minOccurs="0">          <annotation>            <documentation>Pasientens opplysning om første fraværsdag.</documentation>            <documentation>EDA: Satte minOcc til 0 for å kunne kjøre EDIFACT løpet gjennom. Pasientens opplysning om første fraværsdag.</documentation>          </annotation>        </element>        <element name="ForsteKonsultasjon" type="date" minOccurs="0">          <annotation>            <documentation>Legens opplysninger om når første konsultasjon har vært utført.</documentation>          </annotation>        </element>        <element name="DatoTelefonKonsultasjon" type="date" minOccurs="0">          <annotation>            <documentation>Dato for når telefonkonsultasjon er utført.</documentation>          </annotation>        </element>        <element ref="rtv:Varighet" minOccurs="0"/>        <sequence minOccurs="0">          <element name="IkkeJobb" type="boolean" minOccurs="0">            <annotation>              <documentation>Angir om personen ikke kan være på jobb under sykmeldingen. EDAH: minOccurance = 0</documentation>              <documentation>EDA:24.01.03 - opphevet required, has satt MinOcc til 0, pga edifact løpet. Konktroll av felt gjrøes i regelsystemet.</documentation>              <documentation>EDA-04.04.03: Endret fra choice til sequence og minOcc til 0 på elementene under for å få EDIFact gjennom.</documentation>            </annotation>          </element>          <element name="Forutsetninger" minOccurs="0">            <annotation>              <documentation>Personen kan likevel være på jobb i nevnte periode under forutsetning av følgende. EDAH: MinOcc = 0</documentation>            </annotation>            <complexType>              <sequence>                <element name="ReisetilskuddFra" type="date" minOccurs="0">                  <annotation>                    <documentation>Dato fra når reisetilskudd foreslås.</documentation>                  </annotation>                </element>                <element name="AktivSykmeldFra" type="date" minOccurs="0">                  <annotation>                    <documentation>Når aktiv sykmelding foreslås fra.</documentation>                  </annotation>                </element>                <element name="Eventuelt" type="rtv:typeTekst" minOccurs="0">                  <annotation>                    <documentation>Eventuelt hva bør personen ikke utføre eller utsettes for ved aktiv sykmelding.</documentation>                  </annotation>                </element>              </sequence>            </complexType>          </element>        </sequence>        <element name="ArbeidsuforBehandlingsdager" type="rtv:typeTekst" minOccurs="0"/>      </sequence>    </complexType>  </element>  <element name="Identifikasjon">    <annotation>      <documentation>For å angi hvordan pasienten er identifisert.</documentation>      <documentation>EDA: Endring: fra choice til sequence og minocc på begge barna (children) til 0 grunnet edifactløpet.For å angi hvordan pasienten er identifisert.   </documentation>    </annotation>    <complexType>      <sequence>        <element name="Kjent" type="boolean" minOccurs="0">          <annotation>            <documentation>Angir om personen er kjent.</documentation>          </annotation>        </element>        <element name="Legitimasjon" type="boolean" minOccurs="0">          <annotation>            <documentation>Angir om legitimasjon er vist.</documentation>          </annotation>        </element>      </sequence>    </complexType>  </element>  <element name="FriskmeldingPrognose">    <annotation>      <documentation>Opplysninger om når personen forventes å bli helt arbeidsfør, eller prognose om fortsatt arbeidsuførhet.</documentation>      <documentation>EDA: Endret Choice til Sequence pga edifactløpet. Opplysninger om når personen forventes å bli helt arbeidsfør, eller prognose om fortsatt arbeidsuførhet.</documentation>    </annotation>    <complexType>      <sequence>        <element ref="rtv:Friskmeldt" minOccurs="0"/>        <element ref="rtv:ArbeidsUfor" minOccurs="0"/>      </sequence>    </complexType>  </element>  <element name="DiagnoseArbeidssituasjon">    <annotation>      <documentation>For å angi om fravær fra arbeid har andre årsaker enn egen sykdom, om sykdommen er svangerskapsrelatert, og evt. angivelse av skadedato for yrkessykdom/yrkesskade.</documentation>    </annotation>    <complexType>      <sequence>        <element ref="rtv:DiagnoseKodesystem" minOccurs="0"/>        <element name="Svangerskap" type="boolean" minOccurs="0"/>        <element name="AnnenFraversgrunn" type="rtv:typeTekst" minOccurs="0"/>        <element name="Yrkessykdom" type="boolean" minOccurs="0"/>        <element name="Yrkesskade" type="boolean" minOccurs="0"/>        <element name="SkadeDato" type="date" minOccurs="0"/>      </sequence>    </complexType>  </element>  <element name="Merknad" type="rtv:typeTekst">    <annotation>      <documentation>For å angi merknader</documentation>    </annotation>  </element>  <element name="MedisinskeOpplysninger">    <annotation>      <documentation>Del II. For å angi opplysninger om behandling, utredning, tiltak.</documentation>    </annotation>    <complexType>      <sequence>        <element name="Behandling" type="rtv:typeTekst">          <annotation>            <documentation>Opplysninger om symptomer, status presens og pågående behandling.</documentation>          </annotation>        </element>        <element name="Spesifikasjon" type="rtv:typeTekst" minOccurs="0">          <annotation>            <documentation>Hvis pasienten er henvist til utredning eller behandling, kan legen spesifisere type utredning/behandling (f.eks. røntgen, fysioterapi) og angi hvor behandlingen skal foregå (institusjon, poliklinikk, spesialist etc.).</documentation>          </annotation>        </element>        <element ref="rtv:HenvistTil" minOccurs="0" maxOccurs="3"/>      </sequence>    </complexType>  </element>  <element name="Funksjonsvurdering">    <annotation>      <documentation>Del II. Beskriver en forenklet funksjonsvurdering. OBS: Dette feltet gir gjennomslag til personens arbeidsgiver.</documentation>    </annotation>    <complexType>      <sequence>        <element ref="rtv:VurderingFunksjonsevneSm" minOccurs="0" maxOccurs="3"/>        <element name="IkkeUtforeUtsette" type="rtv:typeTekst" minOccurs="0">          <annotation>            <documentation>Angir hva personen ikke bør utføre eller utsettes for.</documentation>          </annotation>        </element>        <element name="Hjelpemiddelbehov" type="boolean" minOccurs="0">          <annotation>            <documentation>Angir om hjelpemiddelbehov bør vurderes.</documentation>          </annotation>        </element>        <element name="Forhold" type="rtv:typeTekst" minOccurs="0">          <annotation>            <documentation>Angir forhold i arbeidsmiljøet som bør drøftes med arbeidsgiveren, evt. forslag til tiltak.</documentation>          </annotation>        </element>        <element name="KontaktBht" type="boolean" minOccurs="0">          <annotation>            <documentation>Angir om det foreslås kontakt med BHT.</documentation>          </annotation>        </element>      </sequence>    </complexType>  </element>  <element name="VurderingFunksjonsevneSm">    <annotation>      <documentation>For å angi pasientens funksjonsevne. EDA 24.01.03: - minOcc var satt til 3, løste den opp pga edifactløpet, må bestemmes om vi skal tvinge journalleverandørene til å ha med i hvertfall 1 hvis Funksjonsvurdering tag er tilstede. </documentation>    </annotation>    <complexType>      <annotation>        <documentation>EDA: Endret fra required til optional for å kunne kjøre EDIFACT løpet.</documentation>      </annotation>      <attribute name="typeVurdering" type="rtv:typeVurdering" use="required">        <annotation>          <documentation>Benyttes for å oppgi hvilken type vurdering som er gjort.Antas personena)  å kunne gjenoppta nåværende arbeidb) å ha behov for tilrettelagt arbeidc) å ha behov for annet arbeid</documentation>        </annotation>      </attribute>      <attribute name="svar" type="rtv:typeJaNei" use="optional"/>      <attribute name="naEtter" type="rtv:typeNaEtter" use="optional"/>    </complexType>  </element>  <element name="Friskmeldt">    <annotation>      <documentation>Inneholder opplysninger om  når pasienten forventes å bli helt arbeidsfør. Klassen er en spesialisering av den abstrakte klassen Friskmelding/prognose.</documentation>    </annotation>    <complexType>      <attribute name="fraDato" type="date" use="required"/>    </complexType>  </element>  <element name="ArbeidsUfor">    <annotation>      <documentation>Inneholder opplysninger om antall uker  pasienten forventes å være fortsatt arbeidsufør. Klassen er en spesialisering av den abstrakte klassen Friskmelding/prognose.</documentation>    </annotation>    <complexType>      <attribute name="fortsattArbeidsufor" type="string" use="required"/>    </complexType>  </element>  <element name="Varighet">    <annotation>      <documentation>Fra dato, til dato</documentation>    </annotation>    <complexType>      <complexContent>        <extension base="rtv:typeVarighetDato">          <attribute name="grad" type="integer" use="required">            <annotation>              <documentation>Grad av uførhet i prosent. Hvis personen er helt arbeidsufør skal tallet være 100.</documentation>            </annotation>          </attribute>        </extension>      </complexContent>    </complexType>  </element>  <element name="HenvistTil">    <annotation>      <documentation>Angir om personen er eller vil bli henvist til utredning, behandling og/eller anbefalt til kjøp av helsetjenste og hvilken uke henvisningen gjelder.</documentation>    </annotation>    <complexType>      <attribute name="henvist" type="rtv:typeHenvisning" use="optional">        <annotation>          <documentation>Angir om det er henvist til utredning, og/eller behandling og/eller anbefalt kjøp av helsetjeneste. EDAH: Satt til optional.</documentation>        </annotation>      </attribute>      <attribute name="uke" type="string" use="optional">        <annotation>          <documentation>Angir hvilken uke henvisningen gjelder. EDAH: Satt til string (fra int) og optional.</documentation>        </annotation>      </attribute>    </complexType>  </element>  <complexType name="typeVarighetDato">    <annotation>      <documentation>Varighet mellom to datoer.</documentation>    </annotation>    <attribute name="fraDato" type="date" use="required"/>    <attribute name="tilDato" type="date" use="required"/>  </complexType>  <simpleType name="typeSykmelding">    <annotation>      <documentation>Kodeverdier 1-5</documentation>    </annotation>    <restriction base="nonNegativeInteger">      <enumeration value="1">        <annotation>          <documentation>Første attest for dette sykmeldingstilfellet</documentation>        </annotation>      </enumeration>      <enumeration value="2">        <annotation>          <documentation>Forlengelse av sykmeldingstilfellet</documentation>        </annotation>      </enumeration>      <enumeration value="3">        <annotation>          <documentation>Endringer</documentation>        </annotation>      </enumeration>      <enumeration value="4">        <annotation>          <documentation>Informasjon i Del II</documentation>        </annotation>      </enumeration>      <enumeration value="5">        <annotation>          <documentation>Legeerklæring ved sykefravær i mer enn 8 uker er sendt</documentation>        </annotation>      </enumeration>    </restriction>  </simpleType>  <simpleType name="typeJaNeiVetikke">    <annotation>      <documentation>Kodeverdier 1-3</documentation>    </annotation>    <restriction base="nonNegativeInteger">      <enumeration value="1">        <annotation>          <documentation>Ja</documentation>        </annotation>      </enumeration>      <enumeration value="2">        <annotation>          <documentation>Nei</documentation>        </annotation>      </enumeration>      <enumeration value="3">        <annotation>          <documentation>Vet ikke</documentation>        </annotation>      </enumeration>    </restriction>  </simpleType>  <simpleType name="typeHenvisning">    <annotation>      <documentation>Kodeverdier 1-3</documentation>    </annotation>    <restriction base="nonNegativeInteger">      <enumeration value="1">        <annotation>          <documentation>Henvist til utredning</documentation>        </annotation>      </enumeration>      <enumeration value="2">        <annotation>          <documentation>Henvist til behandling</documentation>        </annotation>      </enumeration>      <enumeration value="3">        <annotation>          <documentation>Anbefalt til kjøp av helsetjeneste</documentation>        </annotation>      </enumeration>    </restriction>  </simpleType>  <simpleType name="typeVurdering">    <annotation>      <documentation>Kodeverdier 1-3</documentation>    </annotation>    <restriction base="nonNegativeInteger">      <enumeration value="1">        <annotation>          <documentation>Antas personen å kunne gjennoppta nåværende arbeid</documentation>        </annotation>      </enumeration>      <enumeration value="2">        <annotation>          <documentation>Antas personen å ha behov for tilrettelagt arbeid</documentation>        </annotation>      </enumeration>      <enumeration value="3">        <annotation>          <documentation>Antas personen å ha behov for annet arbeid</documentation>        </annotation>      </enumeration>    </restriction>  </simpleType>  <!--= = = = LEGEERKLÆRING = = = =-->  <element name="Legeerklaring">    <annotation>      <documentation>Spesialisering av den abstrakte klassen Melding, til  klassen Legeerklæring ved arbeidsuførhet.</documentation>    </annotation>    <complexType>      <sequence>        <element ref="rtv:LegeerklaringGjelder" maxOccurs="4"/>        <element ref="rtv:Pasientopplysninger"/>        <element ref="rtv:PlanUtredBehandle" minOccurs="0"/>        <element ref="rtv:DiagnoseArbeidsuforhet"/>        <element ref="rtv:ForslagTiltak"/>        <element ref="rtv:VurderingFunksjonsevne" minOccurs="0"/>        <element ref="rtv:Prognose"/>        <element ref="rtv:ArsakssammenhengLegeerklaring" minOccurs="0"/>        <element ref="rtv:ForbeholdLegeerklaring" minOccurs="0"/>        <element ref="rtv:AndreOpplysninger" minOccurs="0"/>        <element ref="rtv:Legeunderskrift"/>        <element ref="rtv:Kontakt" minOccurs="0" maxOccurs="unbounded"/>        <element ref="ds:Signature" minOccurs="0"/>      </sequence>      <attribute name="formOfLanguage" type="rtv:typeFormOfLanguage" use="required"/>      <attribute name="sendersId" type="string" use="optional"/>      <attribute name="receiversId" type="string" use="optional"/>      <attribute name="schemaVersion" type="string" use="required" fixed="IA 08-07.08 Fastsatt 10.99 Elektronisk 1.1"/>      <attribute name="messageId" type="string" use="required"/>      <attribute name="genDate" type="dateTime" use="required"/>    </complexType>  </element>  <element name="VurderingFunksjonsevne">    <annotation>      <documentation>For å beskrive hvordan pasientens generelle funksjonsevne er nedsatt. EDAH: minOccurance = 0</documentation>    </annotation>    <complexType>      <sequence>        <element ref="rtv:Arbeidssituasjon" minOccurs="0" maxOccurs="unbounded"/>        <element ref="rtv:VurderingArbeidsevne" minOccurs="0"/>        <element name="Funksjonsevne" type="rtv:typeTekst" minOccurs="0"/>        <element name="KravArbeid" type="rtv:typeTekst" minOccurs="0"/>      </sequence>    </complexType>  </element>  <element name="ArsakssammenhengLegeerklaring" type="rtv:typeTekst">    <annotation>      <documentation>For å angi hvor stor betydning funksjonsnedsettelsen har for at arbeidsevnen er nedsatt.</documentation>    </annotation>  </element>  <element name="ForbeholdLegeerklaring">    <annotation>      <documentation>For å angi om det er noe i legeerklæringen som pasienten ikke bør få vite av medisinske grunner. EDAH: minOccurance = 0</documentation>    </annotation>    <complexType>      <sequence>        <element name="BorTilbakeholdes" type="rtv:typeTekst" minOccurs="0"/>      </sequence>      <attribute name="tilbakeholdInnhold" type="rtv:typeJaNei" use="optional"/>    </complexType>  </element>  <element name="AndreOpplysninger">    <annotation>      <documentation>For å angi andre opplysninger.</documentation>    </annotation>    <complexType>      <sequence>        <element name="Opplysning" type="rtv:typeTekst"/>      </sequence>      <attribute name="onskesKopi" type="rtv:typeJaNei" use="optional"/>    </complexType>  </element>  <element name="Kontakt">    <annotation>      <documentation>For å angi hvem legen ønsker at trygdekontoret skal ta kontakt med.</documentation>    </annotation>    <complexType>      <attribute name="kontakt" type="rtv:typeKontakt" use="required"/>      <attribute name="annenInstans" type="string" use="optional"/>    </complexType>  </element>  <element name="LegeerklaringGjelder">    <annotation>      <documentation>For å angi om legeerklæringen gjelder sykefravær i mer enn 8 uker, rehabiliteringspenger, yrkesrettet attføring eller uførepensjon.  Det benyttes en forekomst av klassen for hver valgmulighet (tilsvarer et kryss på skjemaet). Hvis både felt 0.1 og felt 0.3 er avkrysset må meldingen inneholde to forekomster av klassen.'</documentation>    </annotation>    <complexType>      <attribute name="typeLegeerklaring" type="rtv:typeLegeerklaring"/>    </complexType>  </element>  <element name="Prognose">    <annotation>      <documentation>For å angi prognose for bedring av arbeidsevnen, varighet av sykdom/skade, varighet av funksjonsnedsettelse, varighet av nedsatt arbeidsevne.</documentation>    </annotation>    <complexType>      <sequence>        <element name="AntattVarighet" type="rtv:typeTekst" minOccurs="0"/>        <element name="VarighetFunksjonsnedsettelse" type="rtv:typeTekst" minOccurs="0"/>        <element name="VarighetNedsattArbeidsevne" type="rtv:typeTekst" minOccurs="0"/>      </sequence>      <attribute name="bedreArbeidsevne" type="rtv:typeJaNei"/>    </complexType>  </element>  <element name="VurderingArbeidsevne">    <annotation>      <documentation>For å angi vurdering av arbeidsevne, begrensninger i forhold til arbeidet pasienten er sykmeldt fra og begrensninger ved vurdering av alternativt arbeid eller yrke.</documentation>    </annotation>    <complexType>      <sequence>        <element name="IkkeGjore" type="rtv:typeTekst" minOccurs="0"/>        <element name="HensynAnnetYrke" type="rtv:typeTekst" minOccurs="0"/>      </sequence>      <attribute name="gjenopptaArbeid" type="rtv:typeJaNei" use="optional"/>      <attribute name="narGjenopptaArbeid" type="rtv:typeNaEtter" use="optional"/>      <attribute name="taAnnetArbeid" type="rtv:typeJaNei" use="optional"/>      <attribute name="narTaAnnetArbeid" type="rtv:typeNaEtter" use="optional"/>    </complexType>  </element>  <element name="Arbeidssituasjon">    <annotation>      <documentation>For å angi type arbeid og hvilke krav som stilles. EDAH: minOccurance = 0</documentation>    </annotation>    <complexType>      <attribute name="arbeidssituasjon" type="rtv:typeArbeidssituasjon" use="optional"/>      <attribute name="annenArbeidssituasjon" type="string" use="optional"/>    </complexType>  </element>  <element name="DiagnoseArbeidsuforhet">    <annotation>      <documentation>For å angi dato for når sykmeldingsperioden begynte i dette sykmeldingstilfellet.</documentation>    </annotation>    <complexType>      <sequence>        <element ref="rtv:DiagnoseKodesystem"/>        <element ref="rtv:VurderingYrkesskade" minOccurs="0"/>        <element name="StatusPresens" type="rtv:typeTekst"/>        <element name="SymptomerBehandling" type="rtv:typeTekst"/>      </sequence>      <attribute name="arbeidsuforFra" type="date"/>    </complexType>  </element>  <element name="PlanUtredBehandle">    <annotation>      <documentation>Plan, utredning og behandling.</documentation>    </annotation>    <complexType>      <sequence>        <element ref="rtv:HenvistUtredning" minOccurs="0"/>        <element ref="rtv:HenvistBehandling" minOccurs="0"/>        <element name="NyeLegeopplysninger" type="string" minOccurs="0"/>        <element name="IkkeVidereBehandling" type="rtv:typeT